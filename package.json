{
  "name": "@travi/node-app-engine-standard-scaffolder",
  "description": "host scaffolder for node.js applications hosted on app-engine standard",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/"
  ],
  "repository": "travi/node-app-engine-standard-scaffolder",
  "bugs": "https://github.com/travi/node-app-engine-standard-scaffolder/issues",
  "homepage": "https://npm.im/@travi/node-app-engine-standard-scaffolder",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "lint:md": "remark . --frail",
    "lint:js": "eslint . --cache",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null",
    "test:unit:base": "mocha 'src/**/*-test.js'",
    "test:unit": "nyc run-s test:unit:base",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "clean": "rimraf lib/",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/register": "7.18.9",
    "@form8ion/javascript": "7.2.0",
    "@travi/any": "2.1.5",
    "@travi/babel-preset": "3.0.90",
    "@travi/eslint-config": "3.0.5",
    "@travi/eslint-config-mocha": "1.0.18",
    "ban-sensitive-files": "1.9.18",
    "chai": "4.3.6",
    "codecov": "3.8.3",
    "commitlint-config-travi": "1.4.16",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.1",
    "mocha": "10.0.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "remark-cli": "11.0.0",
    "remark-preset-lint-travi": "4.0.1",
    "rimraf": "3.0.2",
    "rollup": "2.77.0",
    "rollup-plugin-auto-external": "2.0.0",
    "sinon": "14.0.0"
  },
  "dependencies": {
    "@travi/cli-messages": "^1.0.3",
    "write-yaml": "^1.0.0"
  },
  "peerDependencies": {
    "@form8ion/javascript": ">=2.0.0"
  }
}
