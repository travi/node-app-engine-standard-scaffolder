{
  "name": "@travi/node-app-engine-standard-scaffolder",
  "description": "host scaffolder for node.js applications hosted on app-engine standard",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/node-app-engine-standard-scaffolder",
  "bugs": "https://github.com/travi/node-app-engine-standard-scaffolder/issues",
  "homepage": "https://npm.im/@travi/node-app-engine-standard-scaffolder",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "sideEffects": false,
  "scripts": {
    "lint:md": "remark . --frail",
    "lint:js": "eslint . --cache",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null",
    "test:unit:base": "mocha 'src/**/*-test.js'",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "pretest": "run-s build",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "clean": "rimraf lib/",
    "prebuild": "run-s clean",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "prepare": "husky install",
    "lint:publish": "publint --strict"
  },
  "files": [
    "lib/"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "dependencies": {
    "@travi/cli-messages": "^1.0.3",
    "write-yaml": "^1.0.0"
  },
  "devDependencies": {
    "@babel/register": "7.27.1",
    "@form8ion/javascript": "14.3.2",
    "@travi/any": "3.1.2",
    "@travi/babel-preset": "3.0.140",
    "@travi/eslint-config": "4.0.5",
    "@travi/eslint-config-mocha": "1.0.25",
    "ban-sensitive-files": "1.10.7",
    "c8": "10.1.3",
    "chai": "4.5.0",
    "commitlint-config-travi": "1.4.56",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "husky": "9.1.7",
    "mocha": "11.2.2",
    "npm-run-all2": "8.0.1",
    "publint": "0.3.12",
    "remark-cli": "12.0.1",
    "remark-preset-lint-travi": "6.0.8",
    "rimraf": "6.0.1",
    "rollup": "4.40.1",
    "rollup-plugin-auto-external": "2.0.0",
    "sinon": "20.0.0"
  },
  "peerDependencies": {
    "@form8ion/javascript": ">=2.0.0"
  }
}
