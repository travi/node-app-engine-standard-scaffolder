{
  "name": "@travi/node-app-engine-standard-scaffolder",
  "description": "host scaffolder for node.js applications hosted on app-engine standard",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "type": "module",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/node-app-engine-standard-scaffolder",
  "bugs": "https://github.com/travi/node-app-engine-standard-scaffolder/issues",
  "homepage": "https://npm.im/@travi/node-app-engine-standard-scaffolder",
  "main": "./lib/index.js",
  "exports": "./lib/index.js",
  "sideEffects": false,
  "scripts": {
    "lint:md": "remark . --frail",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "run-s 'lint:js -- --fix'",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null",
    "test:unit:base": "DEBUG=any vitest run",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "pretest": "run-s build",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "clean": "rimraf lib/",
    "prebuild": "run-s clean",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "prepare": "husky install",
    "lint:publish": "publint --strict",
    "lint:lockfile": "lockfile-lint",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "pretest:integration:base": "run-s build",
    "test:integration:base": "NODE_OPTIONS=--enable-source-maps DEBUG=any cucumber-js test/integration",
    "test:integration:debug": "DEBUG=test run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "test:integration:wip:debug": "DEBUG=test run-s 'test:integration:wip'",
    "test:integration:focus": "run-s 'test:integration:base -- --profile focus'",
    "lint:gherkin": "gherkin-lint --config=.gherkin-lintrc.json"
  },
  "files": [
    "lib/"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "packageManager": "npm@11.10.1+sha512.c286afb98d8e803150d4afed07d407b14b96f7cf6729fbec29337f879a86c92fb7f8186f5cdfc30364cdcf1e7af496857d3aab113e5b110347c1584593e53582",
  "dependencies": {
    "@travi/cli-messages": "^1.0.3",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@babel/register": "7.28.6",
    "@cucumber/cucumber": "12.6.0",
    "@form8ion/core": "4.11.0",
    "@form8ion/eslint-config-vitest": "1.1.0",
    "@form8ion/javascript": "14.6.0",
    "@travi/any": "3.1.3",
    "@travi/babel-preset": "3.0.146",
    "@travi/eslint-config": "5.0.12",
    "@travi/eslint-config-cucumber": "1.0.6",
    "ban-sensitive-files": "1.10.11",
    "c8": "10.1.3",
    "commitlint-config-travi": "1.4.59",
    "cross-env": "10.1.0",
    "cz-conventional-changelog": "3.3.0",
    "gherkin-lint": "4.2.4",
    "husky": "9.1.7",
    "lockfile-lint": "5.0.0",
    "mock-fs": "5.5.0",
    "npm-run-all2": "8.0.4",
    "publint": "0.3.17",
    "remark-cli": "12.0.1",
    "remark-preset-lint-travi": "6.0.8",
    "rimraf": "6.1.3",
    "rollup": "4.58.0",
    "rollup-plugin-auto-external": "2.0.0",
    "vitest": "4.0.18",
    "vitest-when": "0.10.0"
  }
}
